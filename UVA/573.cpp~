#include <cstdio>

int main(void){
  int h, u, d, i, flag;
  double ans, fatigue, f, deslocamento;
  while(scanf("%d %d %d %lf", &h, &u, &d, &f), h != 0){
    flag = 1;
    ans = 0;
    for(i = 0; flag; i++){
      fatigue = (u * (f/100)) * i;
      deslocamento = u - fatigue;
      if(deslocamento < 0) deslocamento = 0;
      ans += deslocamento - d;
      if(ans < 0 || (ans == 0 && u - fatigue < 0) || ans + d > h) flag = 0;
    }
    if(u - d - fatigue < 0) printf("failure on day %d\n", i);
    else printf("success on day %d\n", i);
  }
  return 0;
}
